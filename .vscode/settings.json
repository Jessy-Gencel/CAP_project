{
  // uncomment entries once all libraries have been installed via 'npm install'
  "eslint.validate": [
    "cds",
    "csn",
    "csv",
    "csv (semicolon)",
    "tab",
    "tsv"
  ],
  "cds": {
    "requires": {
      "db": "sqlite"
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.cds": "cds",
  }
}
